<launch>
<rosparam command="load" file="$(find smros)/config/default.yaml" />
<node name="kittireader" pkg="smros" type="kittireader" output="screen" />
<node name="perception" pkg="smros" type="perception" output="screen" />
<node name="odometry" pkg="smros" type="odometry" output="screen" />
<node name="laserloop" pkg="smros" type="laserloop" output="screen" />
<node name="semanticmap" pkg="smros" type="semanticmap" output="screen" />
<node name="rviz" pkg="rviz" type="rviz" args="-d $(find smros)/config/rviz_config_laser.rviz" output="screen" />
</launch>